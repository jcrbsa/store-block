{
  "name": "eslint-config-vtex",
  "version": "12.2.1",
  "description": "VTEX's eslint config",
  "main": "index.js",
  "scripts": {
    "eslint-check": "eslint --print-config index.js | eslint-config-prettier-check",
    "dump-config": "eslint --print-config index.js > config-dump.json",
    "version": "chan release $npm_package_version && git add CHANGELOG.md && git commit --amend --no-edit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtex/javascript.git"
  },
  "keywords": [
    "eslint",
    "config",
    "vtex"
  ],
  "contributors": [
    "Breno Calazans <breno@vtex.com.br>",
    "Christian Kaisermann <christian.andrade@vtex.com.br>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtex/javascript/issues"
  },
  "homepage": "https://github.com/vtex/javascript#readme",
  "files": [
    "index.js",
    "rules/"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "confusing-browser-globals": "^1.0.9",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-cypress": "^2.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vtex": "^1.0.3"
  },
  "devDependencies": {
    "@vtex/prettier-config": "^0.1.3",
    "eslint": "^6.8.0",
    "prettier": "^1.18.2",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "eslint": "^6.8.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  }
}
