{
  "name": "eslint-config-vtex-react",
  "version": "6.2.1",
  "description": "VTEX's eslint config for React",
  "main": "index.js",
  "scripts": {
    "eslint-check": "eslint --print-config index.js | eslint-config-prettier-check",
    "dump-config": "eslint --print-config index.js > config-dump.json",
    "version": "chan release $npm_package_version && git add CHANGELOG.md && git commit --amend --no-edit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtex/javascript.git"
  },
  "files": [
    "index.js",
    "io.js",
    "native.js",
    "rules/"
  ],
  "keywords": [
    "eslint",
    "config",
    "vtex",
    "react"
  ],
  "contributors": [
    "Breno Calazans <breno@vtex.com.br>",
    "Christian Kaisermann <christian.andrade@vtex.com.br>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtex/javascript/issues"
  },
  "homepage": "https://github.com/vtex/javascript#readme",
  "dependencies": {
    "eslint-config-vtex": "^12.2.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "eslint": "^6.8.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  }
}
